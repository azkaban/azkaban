---
config:
  flow-level-parameter: value

nodes:
  - name: shell_end
    type: noop
    dependsOn:
      - shell_pwd
      - shell_echo

  - name: shell_echo
    type: command
    config:
      command: echo "This is an echoed text."

  - name: shell_pwd
    type: command
    config:
      command: pwd
